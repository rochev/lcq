{
    "name": "rochev/lcq",
    "description": "Laravel Code Quality Library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Rochev",
            "email": "vladimir@rochev.me"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "friendsofphp/php-cs-fixer": "^2.16",
        "illuminate/console": "^6.17",
        "illuminate/support": "^6.17",
        "nunomaduro/larastan": "^0.5.2",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Rochev\\Laravel\\CodeQuality\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Rochev\\Laravel\\CodeQuality\\": "tests/"
        }
    }
}
